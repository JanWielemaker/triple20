<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE rdfs [
  <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY owl  "http://www.w3.org/2002/7/owl#">
  <!ENTITY xsd  "http://www.w3.org/2000/10/XMLSchema#">
  <!ENTITY tool "http://www.swi.psy.uva.nl/mia/tool#">
  <!ENTITY ulan 'http://www.swi.psy.uva.nl/mia/ulan#'>
]>

<rdf:RDF
  xmlns     ="&tool"
  xml:base  ="http://www.swi.psy.uva.nl/mia/tool"
  xmlns:rdf ="&rdf;"
  xmlns:rdfs="&rdfs;"
  xmlns:owl ="&owl;"
  xmlns:xsd ="&xsd;"
  xmlns:tool="&tool;"
  xmlns:ulan="&ulan;"
>

<!-- Render objects in a cell of a tabular -->

<Render
  rdf:ID="list_in_cell"
  tool:context=tabular
  tool:role=object
  tool:class=rdf_object_list_browser>
  <domain rdf:resource="&rdf;List"/>
</Render>

<Render
  rdf:ID="literal_in_cell"
  tool:context=tabular
  tool:role=object
  tool:class=rdf_literal_text>
  <domain rdf:resource="&rdfs;Literal"/>
</Render>

<Render
  rdf:ID="object_in_cell"
  tool:context=tabular
  tool:role=object
  tool:class=rdf_object_text>
  <domain rdf:resource="&rdfs;Resource"/>
</Render>

<!-- Render objects in a list-browser -->

<Render
  rdf:ID="literal_in_browser"
  tool:context=tabular
  tool:role=graphical_browser
  tool:class=rdf_literal_text>
  <domain rdf:resource="&rdfs;Literal"/>
</Render>

<Render
  rdf:ID="object_in_browser"
  tool:context=graphical_browser
  tool:role=object
  tool:class=rdf_object_text>
  <domain rdf:resource="&rdfs;Resource"/>
</Render>

<!-- HIERARCHY RENDERING
     
     These statements describe the relations visualised in the hierarchy
     viewer.  The first one says classes are organised in the subClassOf
     relation and show both subclasses and indivisuals of this class.
     The second states that properties are organised using the subPropertyOf
     relation, their roots connected through the type relation to the
     Property class.  Finally, arbitrary instances are connected to their
     class.
-->

<Render
  rdf:ID="class_in_hierarchy"
  tool:context=rdfs_hierarchy
  tool:class=rdfs_class_node>
  <domain>
    <owl:Class>
      <owl:intersectionOf rdf:parseType=Collection>
	<rdf:Description rdf:about="&rdfs;Class"/>
	<owl:Class>
	  <owl:complementOf>
	     <owl:Class>
	       <owl:oneOf rdf:parseType=Collection>
	         <rdf:Description rdf:about="&rdfs;Class"/>
	       </owl:oneOf>
	     </owl:Class>
	  </owl:complementOf>
	</owl:Class>
      </owl:intersectionOf>
    </owl:Class>
  </domain>
			<!-- Parents -->
  <parent-relation rdf:parseType=Collection>
    <rdf:Description rdf:about="&rdfs;subClassOf"/>
  </parent-relation>
			<!-- Children -->
  <child-relation rdf:parseType=Collection>
    <rdf:Description rdf:about="&rdfs;subClassOf"/>
    <rdf:Description rdf:about="&rdf;type"/>
  </child-relation>
</Render>

<Render
  rdf:ID="class_class_in_hierarchy"
  tool:context=rdfs_hierarchy
  tool:class=rdfs_class_node>
  <domain rdf:resource="&rdfs;Class"/>
			<!-- Parents -->
  <parent-relation rdf:parseType=Collection>
    <rdf:Description rdf:about="&rdfs;subClassOf"/>
  </parent-relation>
			<!-- Children -->
  <child-relation rdf:parseType=Collection>
    <rdf:Description rdf:about="&rdfs;subClassOf"/>
  </child-relation>
</Render>

<Render
  rdf:ID="property_in_hierarchy"
  tool:context=rdfs_hierarchy
  tool:class=rdfs_property_node>
  <domain rdf:resource="&rdf;Property"/>
			<!-- Parents -->
  <parent-relation rdf:parseType=Collection>
    <rdf:Description rdf:about="&rdfs;subPropertyOf"/>
    <rdf:Description rdf:about="&rdf;type"/>
  </parent-relation>
			<!-- Children -->
  <child-relation rdf:parseType=Collection>
    <rdf:Description rdf:about="&rdfs;subPropertyOf"/>
  </child-relation>
</Render>

<Render
  rdf:ID="individual_in_hierarchy"
  tool:context=rdfs_hierarchy
  tool:class=rdfs_individual_node>
  <domain rdf:resource="&rdfs;Resource"/>
 			<!-- Parents -->
  <parent-relation rdf:parseType=Collection>
    <rdf:Description rdf:about="&rdf;type"/>
  </parent-relation>
</Render>


<!-- MIA-specific stuff -->

<Render
  rdf:ID="ulan_year_in_cell"
  tool:context=tabular
  tool:role=object
  tool:class=ulan_timestamp_object_item>
  <domain rdf:resource="&ulan;TimeStamp"/>
</Render>

</rdf:RDF>
